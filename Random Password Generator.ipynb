   "source": [
    "import random \n",
    "passlen = int(input(' Enter the length of password'))\n",
    "s = 'abcdefghijklmnopqrstuvwxyz0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZ!@#$%^&*?'\n",
    "p = \"\".join(random.sample(s, passlen))\n",
    "print(p)"
